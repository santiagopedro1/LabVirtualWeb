    --color-warning-200: 253 236 243; /* ⬅ #fdecf3 */
    --color-warning-300: 251 224 236; /* ⬅ #fbe0ec */
    --color-warning-400: 248 200 221; /* ⬅ #f8c8dd */
    --color-warning-500: 245 177 207; /* ⬅ #F5B1CF */
