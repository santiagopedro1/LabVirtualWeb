:root {
    /* =~= Theme Properties =~= */
    --theme-font-family-base: Roboto, Inter, ui-sans-serif, system-ui,
        -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Helvetica Neue', Arial,
        'Noto Sans', sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji',
        'Segoe UI Symbol', 'Noto Color Emoji';
    --theme-font-family-heading: system-ui;
    --theme-font-color-base: 0 0 0;
    --theme-font-color-dark: 255 255 255;
    --theme-rounded-base: 9999px;
    --theme-rounded-container: 12px;
    --theme-border-base: 1px;
    /* =~= Theme On-X Colors =~= */
    --on-primary: 0 0 0;
    --on-secondary: 0 0 0;
    --on-tertiary: 255 255 255;
    --on-success: 0 0 0;
    --on-warning: 0 0 0;
    --on-error: 255 255 255;
    --on-surface: 255 255 255;
    /* =~= Theme Colors  =~= */
    /* primary | #019B43 */
    --color-primary-50: 217 240 227; /* ⬅ #d9f0e3 */
    --color-primary-100: 204 235 217; /* ⬅ #ccebd9 */
    --color-primary-200: 192 230 208; /* ⬅ #c0e6d0 */
    --color-primary-300: 153 215 180; /* ⬅ #99d7b4 */
    --color-primary-400: 77 185 123; /* ⬅ #4db97b */
    --color-primary-500: 1 155 67; /* ⬅ #019B43 */
    --color-primary-600: 1 140 60; /* ⬅ #018c3c */
    --color-primary-700: 1 116 50; /* ⬅ #017432 */
    --color-primary-800: 1 93 40; /* ⬅ #015d28 */
    --color-primary-900: 0 76 33; /* ⬅ #004c21 */
    /* secondary | #F7B100 */
    --color-secondary-50: 254 243 217; /* ⬅ #fef3d9 */
    --color-secondary-100: 253 239 204; /* ⬅ #fdefcc */
    --color-secondary-200: 253 236 191; /* ⬅ #fdecbf */
    --color-secondary-300: 252 224 153; /* ⬅ #fce099 */
    --color-secondary-400: 249 200 77; /* ⬅ #f9c84d */
    --color-secondary-500: 247 177 0; /* ⬅ #F7B100 */
    --color-secondary-600: 222 159 0; /* ⬅ #de9f00 */
    --color-secondary-700: 185 133 0; /* ⬅ #b98500 */
    --color-secondary-800: 148 106 0; /* ⬅ #946a00 */
    --color-secondary-900: 121 87 0; /* ⬅ #795700 */
    /* tertiary | #BF1022 */
    --color-tertiary-50: 245 219 222; /* ⬅ #f5dbde */
    --color-tertiary-100: 242 207 211; /* ⬅ #f2cfd3 */
    --color-tertiary-200: 239 195 200; /* ⬅ #efc3c8 */
    --color-tertiary-300: 229 159 167; /* ⬅ #e59fa7 */
    --color-tertiary-400: 210 88 100; /* ⬅ #d25864 */
    --color-tertiary-500: 191 16 34; /* ⬅ #BF1022 */
    --color-tertiary-600: 172 14 31; /* ⬅ #ac0e1f */
    --color-tertiary-700: 143 12 26; /* ⬅ #8f0c1a */
    --color-tertiary-800: 115 10 20; /* ⬅ #730a14 */
    --color-tertiary-900: 94 8 17; /* ⬅ #5e0811 */
    /* success | #5FC8D1 */
    --color-success-50: 231 247 248; /* ⬅ #e7f7f8 */
    --color-success-100: 223 244 246; /* ⬅ #dff4f6 */
    --color-success-200: 215 241 244; /* ⬅ #d7f1f4 */
    --color-success-300: 191 233 237; /* ⬅ #bfe9ed */
    --color-success-400: 143 217 223; /* ⬅ #8fd9df */
    --color-success-500: 95 200 209; /* ⬅ #5FC8D1 */
    --color-success-600: 86 180 188; /* ⬅ #56b4bc */
    --color-success-700: 71 150 157; /* ⬅ #47969d */
    --color-success-800: 57 120 125; /* ⬅ #39787d */
